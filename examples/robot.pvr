    <Script base_name="init" group="no group" name_space="__script__" name_suffix="0" persistency="666" server="server1" type="Python">
      <core>
	import VR
	
	if hasattr(VR, 'scene'): VR.scene.destroy()
	VR.scene = VR.Object('scene', 'light')
	
	
	VR.robot = VR.RobotArm("kuka")
	VR.robot.showAnalytics(1)
	
	arm = VR.loadGeometry('robotArm.dae')	
	tool = VR.loadGeometry('greifer.dae', 0)
	tool.setTransform([0.052,0,0.052],[0,0,-1],[0,1,0])
	tool.addTag('grab')
	tool.setScale([0.72,0.72,0.72])
	parts = arm.getChildren(True, 'Transform')[1:]
	parts[4].addChild(tool)
	parts.append(tool)
	parts.append(tool.find('finger1'))
	parts.append(tool.find('finger2'))
	VR.robot.setParts(parts)
	VR.robot.setAxis([1,0,0,2,0])
	VR.robot.setAngleOffsets([0,0,-0.5,0,0])
	VR.robot.setAngleDirections([1,-1,1,1,1])
	VR.robot.setLengths([0.16, 0.271, 0.236, 0.14, 0.026])
	VR.robot.setAngles([0,0,0,0,0])
	VR.scene.addChild(arm)
	
	def addPrimitive(name,params,pos):
		g = VR.Geometry(name)
		g.setPrimitive(params)
		g.setFrom(pos)
		VR.scene.addChild(g)
		return g
		
	ground = addPrimitive('ground', 'Box 3 0.1 3 1 1 1', [0,-0.05,0])
	box = addPrimitive('box', 'Box 0.25 0.25 0.25 1 1 1', [0.4,0.125,0])
	box.setColor('green')
	
	
	
	
	
