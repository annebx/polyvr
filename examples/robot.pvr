	import VR
	from VR.Math import Vec3
	
	if dev.intersect():
		p = Vec3(dev.getIntersection())
		d = Vec3(dev.getIntersectionNormal())
		u = Vec3([0,1,0])
		if abs(d.dot(u)) &gt; 0.9: u = Vec3([1,0,0])
		VR.robot.moveTo( VR.Pose(p,-d,u) )
